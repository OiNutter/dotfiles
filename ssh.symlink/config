#GitHub
Host github.com
  HostName github.com
  User git
  
#RaspberryPi
Host rpi
  HostName openelec
  User root

#Jenkins Server
Host jenkins
	HostName ci.komodohq.co.uk
	User jenkins
	ForwardAgent true
	IdentityFile ~/.ssh/komodo_rsa

#Jenkins Server DevOps
Host jenkins-devops
	HostName ci.komodohq.co.uk
	User devops
	ForwardAgent true
	IdentityFile ~/.ssh/komodo_rsa
